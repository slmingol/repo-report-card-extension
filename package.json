{
  "name": "repo-report-card",
  "displayName": "Code Quality Report Card",
  "description": "Grade repositories and pull requests with Principal Skinner using GitHub Copilot",
  "version": "1.2.6",
  "publisher": "slmingol",
  "repository": {
    "type": "git",
    "url": "https://github.com/slmingol/repo-report-card-extension.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "repo-report-card.analyze",
        "title": "Repo Report Card: Analyze Repositories"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "simple-git": "^3.21.0"
  }
}
